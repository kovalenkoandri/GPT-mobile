{
  "expo": {
    "jsEngine": "hermes",
    "name": "GPT-mobile",
    "slug": "GPT-mobile",
    "scheme": "com.kovalenkoandrii.gptmobile",
    "icon": "./assets/cloud1024.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2f2f3d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "exp.host",
              "pathPrefix": "/@kovalenkoandrii/GPT-mobile"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/cloud1024.png",
        "backgroundColor": "#2f2f3d"
      },
      "package": "com.kovalenkoandrii.GPTmobile",
      "versionCode": 32
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "accessible": true,
      "accessibilityLabel": "AI web app icon"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/41fcefab-6f7d-48cb-9092-0ab3fabde6a3",
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-updates",
        {
          "username": "kovalenkoandrii"
        }
      ]
    ],
    "owner": "kovalenkoandrii"
  }
}
